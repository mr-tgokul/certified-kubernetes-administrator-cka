# Create Pod in Custom Namesapce
# Change the Default Namespace
# View the API resources

cat > example38.yaml
kubectl create -f example38.yaml
kubectl get ns 
kubectl run nginx1 --image=nginx
kubectl run nginx1 --image=nginx --namespace=my-ns1
kubectl run tomcat1 --image=vishymails/tomcatimage:1.0
kubectl get po 
kubectl get po --namespace=my-ns1
cat > example39.yaml
kubectl create -f example39.yaml
kubectl get po --namespace=my-ns1
kubectl get po 
kubectl get po --namespace=my-ns1
kubectl config set-context --current --namespace=my-ns1
kubectl get po
kubectl api-resources 
kubectl api-resources --namespaced=true
kubectl api-resources --namespaced=flase
kubectl api-resources --namespaced=false
kubectl get po 
kubectl get po --all-namesapces
kubectl get po --all-namespaces
kubectl get deployments  --all-namespaces
history
