echo -n 'admin' | base64 > username.txt 
echo -n "password123' | base64 > password.txt
echo -n "password123" | base64 > password.txt
cat username.txt
cat password.txt
kubectl get secrets 
kubectl create secret generic db-user-pass --from-file=./username.txt --from-file=./password.txt
kubectl get secrets 
kubectl describe secrets db-user-pass
cat > example28.yaml
kubectl create -f example28.yaml
kubectl get secrets 
history 
